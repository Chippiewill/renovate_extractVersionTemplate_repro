{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prConcurrentLimit": 5,
  "regexManagers": [
    {
      "fileMatch": ["^version.yml$"],
      "matchStrings": [
        "VERSION__K8s_PROVIDER: '(?<currentValue>.*?)'"
    ],
      "depNameTemplate": "fiveai/terraform-provider-k8s/releases",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "regex:^v(?<major>\\d+)(\\.(?<minor>\\d+))(\\.(?<patch>\\d+))$"
    }
  ],
  "enabledManagers": [
    "regex"
  ]
}
